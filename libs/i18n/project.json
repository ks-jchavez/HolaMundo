{
  "root": "libs/i18n",
  "sourceRoot": "libs/i18n/src",
  "projectType": "library",
  "generators": {},
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/i18n/**/*.ts",
          "libs/i18n/**/*.spec.ts",
          "libs/i18n/**/*.spec.tsx",
          "libs/i18n/**/*.spec.js",
          "libs/i18n/**/*.spec.jsx",
          "libs/i18n/**/*.d.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "libs/i18n/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/libs/i18n"]
    }
  },
  "tags": []
}
